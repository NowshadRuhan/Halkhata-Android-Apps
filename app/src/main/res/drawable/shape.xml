<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="30dp" />
            <stroke android:width="30dp" android:color="#1a10de" />
            <gradient android:angle="90" android:startColor="#3125b4" android:endColor="#687d9a"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="30sp" />
            <stroke android:width="30sp" android:color="#395dbe" />
            <solid android:color="#2c4ac2"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="38dp" />
            <stroke android:width="1dip" android:color="#4de21b" />
            <gradient android:angle="90" android:startColor="#8dba8f" android:endColor="#46cc5c" />
        </shape>
    </item>
</selector>